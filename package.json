{
  "name": "@uidev1116/acms-js-sdk",
  "version": "0.2.0",
  "description": "a-blog cms JavaScript SDK",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/acms-js-sdk.cjs",
  "module": "./dist/es/acms-js-sdk.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/acms-js-sdk.js",
      "require": "./dist/cjs/acms-js-sdk.cjs"
    },
    "./acmsPath": {
      "types": "./dist/types/lib/acmsPath/index.d.ts",
      "import": "./dist/es/acms-path.js",
      "require": "./dist/cjs/acms-path.cjs"
    },
    "./typeGuard": {
      "types": "./dist/types/lib/typeGuard/index.d.ts",
      "import": "./dist/es/type-guard.js",
      "require": "./dist/cjs/type-guard.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uidev1116/acms-js-sdk.git"
  },
  "author": "uidev1116",
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "check-update": "npx -p npm-check-updates -c ncu",
    "prepare": "husky install",
    "prepack": "npm run test:ci && npm run lint && npm run build"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "co": "^4.6.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "node-cmd": "^5.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.4"
  },
  "volta": {
    "node": "24.11.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "tags": [
    "a-blog cms"
  ],
  "keywords": [
    "a-blog cms"
  ]
}
